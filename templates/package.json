{
  "name": "{{ name }}",
  "version": "{{ version }}",
  "description": "{{ description }}",
  "main": "src/app/index.js",
  "author": "{{ author }}",
  "license": "{{ license }}",
  "private": {{ private }},
  "scripts": {
    "start": "npm run copy-files && webpack-dev-server --env.dev",
    "build": "npm run copy-files && webpack --env.dev",
    "build:prod": "npm run copy-files && webpack --env.prod -p",
    "copy-files": "copyfiles -u 1 src/images/** dist",
    "docs": "esdoc -c esdoc.json",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "dependencies": {
    "classnames": "2.2.5",
    "react": "15.4.2",
