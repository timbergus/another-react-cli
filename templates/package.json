    "react-redux": "5.0.2",
    "react-router": "3.0.1",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "superagent": "3.3.2"
  },
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "copyfiles": "1.0.0",
    "css-loader": "0.26.1",
    "esdoc": "0.5.2",
    "eslint": "3.13.1",
    "eslint-plugin-react": "6.9.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.26.0",
    "jest-cli": "18.1.0",
    "json-loader": "0.5.4",
    "node-sass": "4.2.0",
